        "symfony/dependency-injection": "<5.3.7",
        "symfony/dom-crawler": "<5.3.7",
        "symfony/error-handler": "<5.3.7",
        "symfony/event-dispatcher": "<5.3.7",
        "symfony/filesystem": "<5.3.7",
